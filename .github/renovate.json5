{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'reservoir-data/.github//.github/renovate.json5',
  ],
  pep723: {
    managerFilePatterns: [
      '/scripts/update_openapi.py/',
    ],
  },
  packageRules: [
    {
      matchPackageNames: [
        'uv',
        'astral-sh/uv-pre-commit',
      ],
      groupName: 'uv-version',
    },
    {
      matchPackageNames: [
        'tox',
        'tox-uv',
      ],
      groupName: 'tox-version',
    },
  ],
}
